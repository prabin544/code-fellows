  "dependencies": {
    "bootstrap": "^4.6.0",
    "concurrently": "^5.3.0",
    "cookie-session": "^1.4.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.6",
    "mongoose": "^5.11.15",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "react-bootstrap": "^1.4.3"
  }
